# Generated by Chef.
backend = "etcd"

<%- if node['confd']['client_cert'] -%>
client_cert = "<%= node['confd']['client_cert'] %>"
<%- end -%>
<%- if node['confd']['client_key'] -%>
client_key = "<%= node['confd']['client_key'] %>"
<%- end -%>
<%- if node['confd']['confdir'] -%>
confdir = "<%= node['confd']['confdir'] %>"
<%- end -%>
<%- if node['confd']['interval'] -%>
interval = <%= node['confd']['interval'] %>
<%- end -%>
<%- if node['confd']['nodes'] -%>
nodes = [
  <%- node['confd']['nodes'].each do |benode| -%>
  "<%= benode %>",
  <%- end -%>
]
<%- end -%>
<%- if node['confd']['prefix'] -%>
prefix = "<%= node['confd']['prefix'] %>"
<%- end -%>
<%- if node['confd']['scheme'] -%>
scheme = "<%= node['confd']['scheme'] %>"
<%- end -%>
<%- if node['confd']['watch'] -%>
watch = <%= node['confd']['watch'] %>
<%- end -%>

quiet = false
debug = false
verbose = true
